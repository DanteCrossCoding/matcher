t.emit(\"new category\", category);\n  };\n\n  const startMatch = function (category) {\n    setLoading(true);\n    const responseObj = {category: category, user: props.user}\n    socket.emit(\"new match session\", responseObj);\n    console.log(\"matching started\");\n  };\n\n  const sendAnswerSetState = function (answer) {\n    socket.emit(\"answer\", answer);\n    setResponse((prev) => [...prev, `${answer.ans}: ${answer.restaurant}`]);\n  };\n\n  const isLoading = function () {\n    if (loading) {\n      return (\n        <div class=\"spinner\">\n          <h6>Loading Restaurants...</h6>\n          <Spinner animation=\"border\" />\n        </div>\n      )\n    }\n  }\n\n  const handleSelect = (selectedIndex, e) => {\n    if (selectedIndex === 9 && index === 0) {\n      sendAnswerSetState({\n        ans: \"nay\",\n        user: props.user,\n        restaurantPhone: restaurants[index].phone,\n        restaurant: restaurants[index],\n      });\n    } else if (selectedIndex > index) {\n      sendAnswerSetState({\n        ans: \"yay\",\n        user: props.user,\n        restaurantPhone: restaurants[index].phone,\n        restaurant: restaurants[index],\n      });\n    } else {\n      sendAnswerSetState({\n        ans: \"nay\",\n        user: props.user,\n        restaurantPhone: restaurants[index].phone,\n        restaurant: restaurants[index],\n      });\n    }\n    setIndex(index + 1);\n    setRestaurant(restaurants[index + 1]);\n  };\n\n  const matchingStarted = function () {\n    if (restaurant) {\n      const carouselItems = restaurants.map((restaurant, index) => {\n        return (\n          <Carousel.Item key={index}>\n            <img\n              className=\"d-block w-100 carousel-img\"\n              src={restaurant.image_url}\n              alt=\"First slide\"\n            />\n          </Carousel.Item>\n        );\n      });\n      return (\n        <div>\n          <h1>{restaurant.name}</h1>\n          <Carousel\n            indicators={false}\n            interval={null}\n            activeIndex={index}\n            onSelect={handleSelect}\n            fade={true}\n          >\n            {carouselItems}\n          </Carousel>\n          <div className=\"button-row\">\n          </div>\n          <div className=\"rating\">\n            <div className=\"top\" style={topStyle}>\n              <span>★</span>\n              <span>★</span>\n              <span>★</span>\n              <span>★</span>\n              <span>★</span>\n            </div>\n            <div className=\"bottom\">\n              <span>☆</span>\n              <span>☆</span>\n              <span>☆</span>\n              <span>☆</span>\n              <span>☆</span>\n            </div>\n          </div>\n          <div className=\"description\">\n            <table className=\"table\">\n              <tbody>\n                <tr>\n                  <th scope=\"row\">Phone</th>\n                  <td>{restaurant ? restaurant.phone : \"?\"}</td>\n                </tr>\n                <tr>\n                  <th scope=\"row\">Address</th>\n                  <td>{restaurant ? restaurant.address : \"?\"}</td>\n                </tr>\n                <tr>\n                  <th scope=\"row\">City</th>\n                  <td>{restaurant ? restaurant.city : \"?\"}</td>\n                </tr>\n                <tr>\n                  <th scope=\"row\">Price</th>\n                  <td>{restaurant ? restaurant.price : \"?\"}</td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n          <div>\n            {isLoading()}\n          </div>\n          <FormContainer \n            handleSelect={changeCategory}\n            title={\"Select New Category\"}\n          />\n        </div>\n      ); \n    }\n    return (\n      <div>\n        <h1>Welcome!</h1>\n        <FormContainer \n          handleSelect={startMatch}\n          title={\"Select a Category to Start\"}\n        />\n        <div>\n          {isLoading()}\n        </div>\n      </div>\n    )\n  };\n\n  return (\n    <div>\n      {matchingStarted()}\n    </div>\n  );\n}\n","/Users/codemonkey/lighthouse/final_project/matcher/client/src/hooks/mainView.ts",[],"/Users/codemonkey/lighthouse/final_project/matcher/client/src/components/Nav.tsx",[],"/Users/codemonkey/lighthouse/final_project/matcher/client/src/hooks/partnerData.ts",[],"/Users/codemonkey/lighthouse/final_project/matcher/client/src/components/Partner.tsx",[],"/Users/codemonkey/lighthouse/final_project/matcher/client/src/components/Matcher.tsx",[],"/Users/codemonkey/lighthouse/final_project/matcher/client/src/components/PartnerList.tsx",[],"/Users/codemonkey/lighthouse/final_project/matcher/client/src/components/View.tsx",[],"/Users/codemonkey/lighthouse/final_project/matcher/client/src/components/PartnerListItem.tsx",[],"/Users/codemonkey/lighthouse/final_project/matcher/client/src/components/FormContainer.js",[],{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","severity":1,"message":"78","line":7,"column":8,"nodeType":"79","messageId":"80","endLine":7,"endColumn":13},{"ruleId":"77","severity":1,"message":"81","line":9,"column":8,"nodeType":"79","messageId":"80","endLine":9,"endColumn":19},{"ruleId":"77","severity":1,"message":"82","line":11,"column":8,"nodeType":"79","messageId":"80","endLine":11,"endColumn":15},{"ruleId":"77","severity":1,"message":"83","line":15,"column":10,"nodeType":"79","messageId":"80","endLine":15,"endColumn":15},{"ruleId":"84","severity":1,"message":"85","line":77,"column":40,"nodeType":"86","messageId":"87","endLine":77,"endColumn":42},{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","severity":1,"message":"88","line":1,"column":27,"nodeType":"79","messageId":"80","endLine":1,"endColumn":36},{"ruleId":"77","severity":1,"message":"89","line":5,"column":8,"nodeType":"79","messageId":"80","endLine":5,"endColumn":14},{"ruleId":"77","severity":1,"message":"90","line":14,"column":1