lg navbar-dark bg-dark static-top\">\n        <div className=\"container\">\n          <a className=\"navbar-brand\" href=\"/\">\n            Matcher\n          </a>\n          <button\n            className=\"navbar-toggler\"\n            type=\"button\"\n            data-toggle=\"collapse\"\n            data-target=\"#navbarResponsive\"\n            aria-controls=\"navbarResponsive\"\n            aria-expanded=\"false\"\n            aria-label=\"Toggle navigation\"\n          >\n            <span className=\"navbar-toggler-icon\"></span>\n          </button>\n          <div className=\"collapse navbar-collapse\" id=\"navbarResponsive\">\n            <Nav view={view} pageChange={pageChange} />\n          </div>\n        </div>\n      </nav>\n\n      <div className=\"container\">\n        <div className=\"row\">\n          <div className=\"col-lg-12\">\n            {partnerTemp.map((partner) => {\n              if (partner.id === selected) {\n                return <Partner name={partner.name} email={partner.email} />;\n              }\n            })}\n            <Modal show={show} onHide={handleClose}>\n              <Modal.Header closeButton>\n                <Modal.Title>We Got One!</Modal.Title>\n              </Modal.Header>\n              <Modal.Body>\n                Woohoo we found a match! {<a rel=\"noreferrer\" target=\"_blank\" href={`http://www.google.com/search?q=${match}`}>\n                {match}\n                </a>}\n              </Modal.Body>\n              <Modal.Footer>\n                <Button variant=\"secondary\" onClick={handleClose}>\n                  Close\n                </Button>\n              </Modal.Footer>\n            </Modal>\n            <View\n              /* foundMatch={foundMatch} */\n              view={view}\n              select={setSelected}\n              selected={selected}\n              partners={partnerTemp}\n              reset={resetMatch}\n              user={user}\n            />\n          </div>\n        </div>\n      </div>\n    </body>\n  );\n}\n\nexport default App;\n",["70","71"],"/Users/codemonkey/lighthouse/final_project/matcher/client/src/components/Button.js",[],"/Users/codemonkey/lighthouse/final_project/matcher/client/src/index.js",[],"/Users/codemonkey/lighthouse/final_project/matcher/client/src/components/CarouselContainer.js",["72","73","74"],"import React, { useState, useEffect } from \"react\";\nimport \"./Carousel.scss\";\nimport FormContainer from \"./FormContainer\";\nimport { Carousel, Spinner } from \"react-bootstrap\";\nimport Button from \"./Button\";\nimport io from \"socket.io-client\";\n\nconst ENDPOINT = \"http://localhost:9000\";\n\nconst socket = io(ENDPOINT);\n\nexport default function CarouselContainer(props) {\n  const [index, setIndex] = useState(0);\n  const [response, setResponse] = useState([]);\n  const [restaurant, setRestaurant] = useState();\n  const [restaurants, setRestaurants] = useState([]);\n  const [loading, setLoading] = useState(false);\n  const [match, setMatch] = useState('null');\n\n  const rating = restaurant ? restaurant.rating : 0;\n\n  const topStyle = {\n    width: `${rating * 36}%`, //180% is full stars\n  };\n\n  socket.on(\"query response\", (response) => {\n    console.log(\"setting new restaurants...\");\n    setLoading(false);\n    setRestaurant(response[0]);\n    setRestaurants(response);\n    setIndex(0);\n    setMatch('null')\n  });\n\n  socket.on(\"connection\", (response) => {\n    setRestaurants(response);\n    setRestaurant(response[0]);\n    setLoading(false);\n    setIndex(0)\n  });\n\n  socket.on(\"resetCarousel\", () => {\n    console.log('carousel reset')\n    setRestaurant();\n    setRestaurants();\n    setIndex(0);\n    setMatch('null')\n  });\n\n  const changeCategory = function (category) {\n    setLoading(true);\n    socket.emit(\"change category\", category);\n  };\n\n  const startMatch = function (category) {\n    setLoading(true);\n    const responseObj = { category: category, user: props.user };\n    socket.emit(\"new match session\", responseObj);\n    console.log(\"matching started\");\n  };\n\n  const sendAnswerSetState = function (answer) {\n    socket.emit(\"answer\", answer);\n    setResponse((prev) => [...prev, `${answer.ans}: ${answer.restaurant}`]);\n  };\n\n  const isLoading = function () {\n    if (loading) {\n      return (\n        <div class=\"spinner\">\n          <h6>Loading Restaurants...</h6>\n          <Spinner animation=\"border\" />\n        </div>\n      );\n    }\n  };\n\n  const handleSelect = (selectedIndex, e) => {\n    if (selectedIndex === 9 && index === 0) {\n      sendAnswerSetState({\n        ans: \"nay\",\n        user: props.user,\n        restaurantPhone: restaurants[index].phone,\n        restaurant: restaurants[index],\n      });\n    } else if (selectedIndex > index) {\n        sendAnswerSetState({\n          ans: \"yay\",\n          user: props.user,\n          restaurantPhone: restaurants[index].phone,\n          restaurant: restaurants[index],\n        });\n        if (index === 8) {\n          setMatch('false')\n        }\n    } else {\n        sendAnswerSetState({\n          ans: \"nay\",\n          user: props.user,\n          restaurantPhone: restaurants[index].phone,\n          restaurant: restaurants[index],\n        });\n        if (index === 8) {\n          setMatch('false')\n        }\n    }\n    setIndex(index + 1);\n    setRestaurant(restaurants[index + 1]);\n  };\n\n  const matchingStarted = function () {\n    if ((match === 'false')) {\n      return (\n        <div>\n          <h1>No matches found yet</h1>\n          <h3>Keep Matching:</h3>\n          <FormContainer\n            handleSelect={changeCategory}\n            title={\"Select another Category\"}\n          />\n          <h3>\n            Or find a {\n              <a rel=\"noreferrer\" target=\"_blank\" href={`http://www.google.com/search?q=Marriage+counsellor`}>\n              marriage counsellor\n            </a>\n            }\n          </h3>\n          <div>{isLoading()}</div>\n        </div>\n      );\n    } \n    if (restaurant === undefined && match === 'null') {\n      return (\n        <div>\n          <h1>Welcome!</h1>\n          <FormContainer\n            handleSelect={startMatch}\n            title={\"Select a Category to Start\"}\n          />\n          <div>{isLoading()}</div>\n        </div>\n      );\n    }\n    else {\n      const carouselItems = restaurants.map((restaurant, index) => {\n        return (\n          <Carousel.Item key={index}>\n            <img\n              className=\"d-block w-100 carousel-img\"\n              src={restaurant.image_url}\n              alt=\"First slide\"\n            />\n          </Carousel.Item>\n        );\n      });\n      return (\n        <div>\n          <h1>{restaurant.name}</h1>\n          <Carousel\n            indicators={false}\n            interval={null}\n            activeIndex={index}\n            onSelect={handleSelect}\n            fade={true}\n          >\n            {carouselItems}\n          </Carousel>\n          <div className=\"button-row\"></div>\n          <div className=\"rating\">\n            <div className=\"top\" style={topStyle}>\n              <span>★</span>\n              <span>★</span>\n              <span>★</span>\n              <span>★</span>\n              <span>★</span>\n            </div>\n            <div className=\"bottom\">\n              <span>☆</span>\n              <span>☆</span>\n              <span>☆</span>\n              <span>☆</span>\n              <span>☆</span>\n            </div>\n          </div>\n          <div className=\"description\">\n            <table className=\"table\">\n              <tbody>\n                <tr>\n                  <th scope=\"row\">Phone</th>\n                  <td>{restaurant ? restaurant.phone : \"?\"}</td>\n                </tr>\n                <tr>\n                  <th scope=\"row\">Address</th>\n                  <td>{restaurant ? restaurant.address : \"?\"}</td>\n                </tr>\n                <tr>\n                  <th scope=\"row\">City</th>\n                  <td>{restaurant ? restaurant.city : \"?\"}</td>\n                </tr>\n                <tr>\n                  <th scope=\"row\">Price</th>\n                  <td>{restaurant ? restaurant.price : \"?\"}</td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n          <div>{isLoading()}</div>\n          <FormContainer\n            handleSelect={changeCategory}\n            title={\"Select New Category\"}\n          />\n        </div>\n      );\n    }\n  };\n\n  return <div>{matchingStarted()}</div>;\n}\n","/Users/codemonkey/lighthouse/final_project/matcher/client/src/hooks/mainView.ts",[],"/Users/codemonkey/lighthouse/final_project/matcher/client/src/components/Nav.tsx",[],"/Users/codemonkey/lighthouse/final_project/matcher/client/src/hooks/partnerData.ts",[],"/Users/codemonkey/lighthouse/final_project/matcher/client/src/components/Partner.tsx",[],"/Users/codemonkey/lighthouse/final_project/matcher/client/src/components/Matcher.tsx",[],"/Users/codemonkey/lighthouse/final_project/matcher/client/src/components/PartnerList.tsx",[],"/Users/codemonkey/lighthouse/final_project/matcher/client/src/components/View.tsx",[],"/Users/codemonkey/lighthouse/final_project/matcher/client/src/components/PartnerListItem.tsx",[],"/Users/codemonkey/lighthouse/final_project/matcher/client/src/components/FormContainer.js",[],["75","76"],{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","severity":1,"message":"82","line":7,"column":8,"nodeType":"83","messageId":"84","endLine":7,"endColumn":13},{"ruleId":"81","severity":1,"message":"85","line":9,"column":8,"nodeType":"83","messageId":"84","endLine":9,"endColumn":19},{"ruleId":"81","severity":1,"message":"86","line":11,"column":8,"nodeType":"83","messageId":"84","endLine":11,"endColumn":15},{"ruleId":"81","severity":1,"message":"87","line":15,"col